irq_routine
    lda irq_accumulator                             ;
    pha                                             ;
    txa                                             ;
    pha                                             ;
    tya                                             ;
    pha                                             ;
    lda userVIAInterruptFlagRegister                ; User VIA interrupt flag register
    and #$a0                                        ;
    cmp #$a0                                        ;
    bne skip_increment                              ;
    sta userVIAInterruptFlagRegister                ; User VIA interrupt flag register
    inc irq_counter                                 ;
skip_increment
    pla                                             ;
    tay                                             ;
    pla                                             ;
    tax                                             ;
    pla                                             ;
    sta irq_accumulator                             ;
    jmp (previous_irq2_vector_low)                  ;

