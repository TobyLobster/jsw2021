    beq check_for_sound_keypresses                  ;
    dec note_length                                 ;
    bne check_for_sound_keypresses                  ;
    ldx note_number                                 ;
    lda melody_duration_table,X                     ;
nonsense = * + 2
    sta note_length                                 ;
    lda melody_pitch_table,X                        ;
nonsense3
    sta melody_sound_pitch                          ;
    ldx #<melody_sound                              ;
    ldy #>melody_sound                              ; melody_sound
    lda #7                                          ; Generate a sound
    jsr OSWORD                                      ;
    inc note_number                                 ;
    lda note_number                                 ;
    cmp #$25                                        ;
    bne check_for_sound_keypresses                  ;
    lda #0                                          ;
    sta note_number                                 ;
check_for_sound_keypresses
    lda $ec                                         ; Internal key number of most recently pressed key
    cmp #$d1                                        ; "S"
    bne s_not_pressed                               ;
    lda #1                                          ;
    sta music_enabled                               ;
s_not_pressed
    cmp #$90                                        ; "Q"
    bne q_not_pressed                               ;
    lda #0                                          ;
    sta music_enabled                               ;
play_silence
    ldx #<sound_silence                             ;
    ldy #>sound_silence                             ; sound_silence
    lda #7                                          ; Generate a sound
    jsr OSWORD                                      ;
q_not_pressed
    pla                                             ;
    tay                                             ;
    pla                                             ;
    tax                                             ;
    pla                                             ;
    plp                                             ;
    rts                                             ;

