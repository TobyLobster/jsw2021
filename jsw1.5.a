another_entry_point
    lda #$c8                                        ; Read/Write BREAK/ESCAPE effect
    ldx #3                                          ; Normal ESCAPE effect, Memory cleared on next reset
    jsr OSBYTE                                      ;

    ldx #$9f                                        ; Copy $5600-$569f to $0000-$009f
copy_zero_page_values_loop                          ;
    lda initial_zero_page_values,X                  ;
    sta $00,X                                       ;
    dex                                             ;
    bne copy_zero_page_values_loop                  ;
    ldx #0                                          ;
move_memory_loop
    lda copy_from,X                                 ; Copy $4e00-$51ff to $0400-$07ff
    sta copy_to,X                                   ;
    lda copy_from + $0100,X                         ;
    sta copy_to   + $0100,X                         ;
    lda copy_from + $0200,X                         ;
    sta copy_to   + $0200,X                         ;
    lda copy_from + $0300,X                         ;
    sta copy_to   + $0300,X                         ;

    lda copy_to_sound_workspace,X                   ;  and $5300-$55ff to $0800-$0aff
    sta $0800,X                                     ;
    lda copy_to_sound_workspace + $0100,X           ;
    sta $0900,X                                     ;
    lda copy_to_sound_workspace + $0200,X           ;
    sta enemies_sprite,X                            ;
    dex                                             ;
    bne move_memory_loop                            ;

    lda irq2_vector_low                             ;
    sta previous_irq2_vector_low                    ;
    lda irq2_vector_high                            ;
    sta previous_irq2_vector_high                   ;

    lda #<to_irq_routine                            ;
    sta irq2_vector_low                             ;
    lda #>to_irq_routine                            ;
    sta irq2_vector_high                            ;

    lda #$a0                                        ; Enable timer 2
    sta userVIAInterruptEnableRegister              ; User VIA Interrupt control register
    lda #0                                          ;
    sta userVIAAuxiliaryControlRegister             ; Auxiliary control register

    lda #<to_event_routine                          ;
    sta event_interrupt_vector_low                  ;
    lda #>to_event_routine                          ;
    sta event_interrupt_vector_high                 ;

    ldx #4                                          ; vsync event
    lda #$0e                                        ; Enable Event
    jsr OSBYTE                                      ;

    lda #1                                          ;
    sta melody_active                               ;

    lda #6                                          ; R6: Vertical displayed register
    sta crtcAddressRegister                         ; Write to video controller (register number)
    lda #$15                                        ;
    sta crtcAddressWrite                            ; Write to video controller (register value)

    lda #<screen_address_lookup_table               ;
    sta os_row_multiplication_table_low             ;
    lda #>screen_address_lookup_table               ;
    sta os_row_multiplication_table_high            ;
    ldx #0                                          ;
write_text_window_loop
    lda text_window_string,X                        ;
    jsr OSWRCH                                      ;
    inx                                             ;
    cpx #5                                          ;
    bne write_text_window_loop                      ;
    jmp to_real_entry_point                         ;

